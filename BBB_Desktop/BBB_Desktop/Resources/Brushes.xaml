<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="DarkestGrey" Color="#FF464646"/>
    <SolidColorBrush x:Key="MediumGrey" Color="#FF6F6F6F"/>
    <SolidColorBrush x:Key="LightGrey" Color="#FF838383"/>
