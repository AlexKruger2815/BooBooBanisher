        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BBB_Desktop"
        xmlns:controls="clr-namespace:BBB_Desktop.Controls"
        mc:Ignorable="d"
        Title="MainWindow" Height="675" Width="1000">
    <Grid Background="#FF464646">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>

        <controls:HeaderControl Grid.Row="0"/>
        
        <controls:InspectorContol Grid.Row="1"/>
    </Grid>
</Window>
